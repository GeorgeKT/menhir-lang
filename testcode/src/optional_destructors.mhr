#ret:42

var destruct_counter = 0

struct Box:
    ptr: *int

fn ~Box(self):
    destruct_counter += 1
    delete self.ptr

fn main() -> int:
    var ret = 0
    while destruct_counter < 7:
        let opt = ?Box{new 6}
        match opt:
            ?b => 
                ret += *b.ptr 
            nil =>
                ret += 0
    return ret


