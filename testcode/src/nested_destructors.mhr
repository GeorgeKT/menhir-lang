#ret: 42

var destruct_counter = 0

struct Inner:
    ptr: *int

fn ~Inner(self):
    destruct_counter += 1
    delete self.ptr

struct Outer:
    inner: Inner

fn main() -> int:
    var ret = 0
    while destruct_counter < 7: 
        let i = Outer{Inner{new 6}}
        ret += *i.inner.ptr 
    return ret
        
